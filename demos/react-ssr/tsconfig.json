{
  "compilerOptions": {
    "outDir": "./build/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules"
  ]
}